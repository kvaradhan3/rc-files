typeset -F SECONDS

if [ -t 0 ] ; then
    prtiming() {
        e=$3
        e=${e:-$SECONDS}
        print -f ": %25s\t%8.6f -- %8.6f\t%8.6f\n" $1 $2 $e $(( $e - $2))
    }
fi

#
fpath=(
        $ZDOTDIR
        $ZDOTDIR_LOCAL
        $ZDOTDIR_LOCAL/functions 
        $HOME/src/zsh-completions/src
        /usr/local/share/zsh-completions
        $fpath
    )
